<body>
  <header class="titulo2">
    <p>welcome to</p>
    <h3>logbook</h3>
  </header>
<div class="login">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<ul>
  <li>
    <%= f.label :email %>
    <div class="input"><%= f.email_field :email, autofocus: true, value: 'write your email' %></div>
  </li>

  <li>
    <%= f.label :password %>
    <div class="input"><%= f.password_field :password, autocomplete: "off", value:"password" %></div>
  </li>
 </ul>
 <div class="button">
    <%= f.submit "GO" %>
  </div> 
</div>
<div class="rememberMe">
      <% if devise_mapping.rememberable? -%>
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
      <% end -%>
  
<% end %>
</div>
<%= render "devise/shared/links" %>

</body>
